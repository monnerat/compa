<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="@GETTEXT_PACKAGE@">
	<enum id="org.mate.panel.applet.compa.FrameType">
		<value nick="None" value="0" />
		<value nick="In" value="1" />
		<value nick="Out" value="2" />
		<value nick="Etched in" value="3" />
		<value nick="Etched out" value="4" />
	</enum>
	<schema id="org.mate.panel.applet.compa">
		<key name="monitor-command" type="s">
			<default>''</default>
			<summary>Monitor command</summary>
			<description>Command producing text for the applet area</description>
		</key>
		<key name="monitor-markup" type="b">
			<default>false</default>
			<summary>Monitor markup</summary>
			<description>Applet text is markup</description>
		</key>
		<key name="update-period" type="i">
			<default>60</default>
			<summary>Update period (sec)</summary>
			<description>Automatic update period in seconds for the applet area</description>
		</key>
		<key name="tooltip-command" type="s">
			<default>''</default>
			<summary>Tooltip command</summary>
			<description>Command producing text for the tootip</description>
		</key>
		<key name="tooltip-markup" type="b">
			<default>false</default>
			<summary>Tooltip markup</summary>
			<description>Tooltip text is markup</description>
		</key>
		<key name="click-command" type="s">
			<default>''</default>
			<summary>Click command</summary>
			<description>Command to be executed by clicking the applet</description>
		</key>
		<key name="frame-type" enum="org.mate.panel.applet.compa.FrameType">
			<default>'None'</default>
			<summary>Embossing</summary>
			<description>Applet area shadow type</description>
		</key>
		<key name="label-color" type="s">
			<default>'#00000000'</default>
			<summary>Background color</summary>
			<description>Applet area background color and transparency</description>
		</key>
		<key name="padding" type="i">
			<default>0</default>
			<summary>Padding</summary>
			<description>Applet area left and right padding (#pixels)</description>
		</key>
	</schema>
</schemalist>
